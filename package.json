{
  "name": "create-sld-app",
  "version": "1.0.1",
  "description": "A interactive cli that generates the boilerplate as per your projects requirement",
  "type": "module",
  "main": "lib/index.js",
  "bin": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts --fix",
    "format": "prettier --write --parser typescript '**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^3.1.1",
    "@inquirer/select": "^1.2.11",
    "chalk": "^5.3.0",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm format",
      "pnpm lint"
    ]
  }
}
